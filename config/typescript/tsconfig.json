{
  "compilerOptions": {
    "strict": true,

    "target": "esnext",
    "module": "esnext",
    "jsx": "react-jsx",

    "declaration": true,
    "declarationMap": true,

    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
